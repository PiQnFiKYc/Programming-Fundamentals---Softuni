using System;

namespace Employee_Data
{
    public class Program
    {
        public static void Main(string[] args)
        {

            string firstName = "Amanda";
            string lastName = "Jonson";

            sbyte ageOfEmployer = 27;
            char genderOfEmployer = 'f';
            long employeeID = 8306112507;
            int employeeNumber = 27563571;
            Console.WriteLine("First name: {0}",firstName);
            Console.WriteLine("Last name: {0}",lastName);
            Console.WriteLine("Age: {0}",ageOfEmployer);
            Console.WriteLine("Gender: {0}",genderOfEmployer);
            Console.WriteLine("Personal ID: {0}", employeeID);
            Console.WriteLine("Unique Employee number: {0}",employeeNumber);
            
        }
    }
}
