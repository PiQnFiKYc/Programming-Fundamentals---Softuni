using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Practice_Characters_and_Strings
{
    class Program
    {
        static void Main(string[] args)
        {
            string firstprint = "Software University";
            char b = 'B';
            char y = 'y';
            char e = 'e';
            string lastprint = "I love programming";
            Console.WriteLine(firstprint);
            Console.WriteLine(b);
            Console.WriteLine(y);
            Console.WriteLine(e);
            Console.WriteLine(lastprint);
        }
    }
}
